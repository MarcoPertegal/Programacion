package ejemplofor;

import java.util.Iterator;

public class Principal 
{

	public static void main(String[] args) 
	{
		int tope1=3,tope2=3, num=0;
		
		for(int i=0; i<tope1; i++) {
			System.out.println("  ");
			for (int j=0; j<tope2; j++) {
				num++;
				
					System.out.print("  "+num);
		}

	}

	}
	
}
	
	
